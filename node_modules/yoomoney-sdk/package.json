{
  "name": "yoomoney-sdk",
  "version": "2.2.0",
  "description": "â­ Typed YooMoney Wallet SDK for NodeJS. Supported API's: Auth, Wallet & Notifications",
  "main": "./dist/index.umd.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "type": "commonjs",
  "sideEffects": false,
  "packageManager": "npm@11.6.2",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.umd.js",
      "import": "./dist/index.mjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=10"
  },
  "directories": {
    "lib": "./dist",
    "doc": "./docs",
    "example": "./examples",
    "test": "./src/__tests__"
  },
  "scripts": {
    "test": "vitest",
    "build:dist": "rimraf dist && vite build",
    "build:docs": "typedoc",
    "build": "npm run build:dist && npm run build:docs",
    "prepack": "npm run build",
    "lint": "eslint src/*",
    "prettify": "prettier --write src/**/*"
  },
  "files": [
    "dist",
    "docs",
    "src",
    "examples",
    "coverage",
    "README.md",
    "CONTRIBUTING.md",
    "LICENSE.txt",
    "SECURITY.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexXanderGrib/yoomoney-sdk.git"
  },
  "keywords": [
    "yoomoney",
    "yoo",
    "yandex",
    "money",
    "wallet",
    "auth",
    "payments",
    "rest",
    "api",
    "sdk",
    "typed",
    "typescript",
    "node",
    "nodejs",
    "backend"
  ],
  "author": {
    "name": "Alexander Gribochkin",
    "email": "me@alexxgrib.me",
    "url": "https://alexxgrib.me"
  },
  "maintainers": [
    {
      "name": "Alexander Gribochkin",
      "email": "me@alexxgrib.me",
      "url": "https://alexxgrib.me"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlexXanderGrib/yoomoney-sdk/issues",
    "email": "me@alexxgrib.me"
  },
  "homepage": "https://github.com/AlexXanderGrib/yoomoney-sdk#readme",
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/express": "^5.0.3",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^24.7.0",
    "@vitest/coverage-v8": "3.2.4",
    "dotenv": "^17.2.3",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unicorn": "^61.0.2",
    "express": "^5.1.0",
    "glob": "^11.0.3",
    "globals": "^16.4.0",
    "inquirer": "^12.9.6",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "typedoc": "^0.28.13",
    "typedoc-plugin-markdown": "^4.9.0",
    "typedoc-plugin-missing-exports": "^4.1.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0",
    "untun": "^0.1.3",
    "vite": "^7.1.9",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "redirect-form-builder": "^1.0.0"
  }
}